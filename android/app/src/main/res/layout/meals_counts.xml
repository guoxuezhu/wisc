<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/distance_2"
        android:background="#FFFFFFFF" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/distance_66"
        android:orientation="horizontal">

        <View
            android:layout_width="@dimen/distance_2"
            android:layout_height="match_parent"
            android:background="#FFFFFFFF" />

        <TextView
            android:id="@+id/breakfast_count"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="早餐"
            android:textColor="@color/kaoqingText"
            android:textSize="@dimen/text_size_24" />

        <View
            android:layout_width="@dimen/distance_2"
            android:layout_height="match_parent"
            android:background="#FFFFFFFF" />

        <TextView
            android:id="@+id/breakfast_use_count"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="已用"
            android:textColor="@color/kaoqingText"
            android:textSize="@dimen/text_size_24" />

        <View
            android:layout_width="@dimen/distance_2"
            android:layout_height="match_parent"
            android:background="#FFFFFFFF" />

        <TextView
            android:id="@+id/breakfast_Surplus_count"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="余额"
            android:textColor="@color/kaoqingText"
            android:textSize="@dimen/text_size_24" />


        <View
            android:layout_width="@dimen/distance_2"
            android:layout_height="match_parent"
            android:background="#FFFFFFFF" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/distance_2"
        android:background="#FFFFFFFF" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/distance_66"
        android:orientation="horizontal">

        <View
            android:layout_width="@dimen/distance_2"
            android:layout_height="match_parent"
            android:background="#FFFFFFFF" />

        <TextView
            android:id="@+id/lunch_count"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="午餐"
            android:textColor="@color/kaoqingText"
            android:textSize="@dimen/text_size_24" />

        <View
            android:layout_width="@dimen/distance_2"
            android:layout_height="match_parent"
            android:background="#FFFFFFFF" />

        <TextView
            android:id="@+id/lunch_use_count"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="已用"
            android:textColor="@color/kaoqingText"
            android:textSize="@dimen/text_size_24" />

        <View
            android:layout_width="@dimen/distance_2"
            android:layout_height="match_parent"
            android:background="#FFFFFFFF" />

        <TextView
            android:id="@+id/lunch_Surplus_count"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="余额"
            android:textColor="@color/kaoqingText"
            android:textSize="@dimen/text_size_24" />


        <View
            android:layout_width="@dimen/distance_2"
            android:layout_height="match_parent"
            android:background="#FFFFFFFF" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/distance_2"
        android:background="#FFFFFFFF" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/distance_66"
        android:orientation="horizontal">

        <View
            android:layout_width="@dimen/distance_2"
            android:layout_height="match_parent"
            android:background="#FFFFFFFF" />

        <TextView
            android:id="@+id/dinner_count"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="晚餐"
            android:textColor="@color/kaoqingText"
            android:textSize="@dimen/text_size_24" />

        <View
            android:layout_width="@dimen/distance_2"
            android:layout_height="match_parent"
            android:background="#FFFFFFFF" />

        <TextView
            android:id="@+id/dinner_use_count"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="已用"
            android:textColor="@color/kaoqingText"
            android:textSize="@dimen/text_size_24" />

        <View
            android:layout_width="@dimen/distance_2"
            android:layout_height="match_parent"
            android:background="#FFFFFFFF" />

        <TextView
            android:id="@+id/dinner_Surplus_count"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="余额"
            android:textColor="@color/kaoqingText"
            android:textSize="@dimen/text_size_24" />


        <View
            android:layout_width="@dimen/distance_2"
            android:layout_height="match_parent"
            android:background="#FFFFFFFF" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/distance_2"
        android:background="#FFFFFFFF" />
</LinearLayout>